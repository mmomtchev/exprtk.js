{
    "env": {
        "projectIncludePaths": [
            "${workspaceFolder}/deps/exprtk"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${projectIncludePaths}",
                "/usr/include/node",
                "${default}"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "gnu++14"
        },
        {
            "name": "OSX",
            "includePath": [
                "${projectIncludePaths}",
                "/usr/local/include/node",
                "${default}"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c99",
            "cppStandard": "gnu++14"
        },
        {
            "name": "Win32",
            "includePath": [
                "${projectIncludePaths}",
                "${default}"
            ],
            "compilerPath": "cl.exe",
            "cStandard": "c99",
            "cppStandard": "c++14"
        }
    ],
    "version": 4
}